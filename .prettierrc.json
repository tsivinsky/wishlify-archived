{
  "arrowParens": "always",
  "semi": true,
  "tabWidth": 2,
  "singleQuote": false,
  "useTabs": false,
  "jsxSingleQuote": false,
  "endOfLine": "lf",
  "trailingComma": "es5",
  "importOrder": [
    "^(react(-dom)?)(/(.*))?$",
    "^(next)(/(.*))?$",
    "^@wishlify(/(.*))?$",
    "<THIRD_PARTY_MODULES>",
    "^@/api/(.*)$",
    "^@/stores/(.*)$",
    "^@/hooks/(.*)$",
    "^@/(features|utils)/(.*)$",
    "^@/(layouts|components)/(.*)$",
    "^@/pages/(.*)$",
    "^@/types/(.*)$",
    "^@/styles/(.*)$",
    "^@/images/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
